<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anysou.as_logger">

    <application
        android:name="com.anysou.aslogger.ASLogApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ShowLogActivity" android:label="字符串TextView方式显示日志" />
        <activity android:name=".ShowLogByListViewAcitvity" android:label="字符串数组ListView方式显示日志"></activity>
    </application>

</manifest>